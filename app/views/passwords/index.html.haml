%h1
  Passwords
%table.passwords
  %thead
    %tr
      %th
        Link
      %th
        Name
      %th
        Login
      %th
        Password
  - @passwords.each do |item|
    %tr.item
      %td
        =item.link
      %td
        =item.name
      %td
        =item.login
      %td
        =item.password